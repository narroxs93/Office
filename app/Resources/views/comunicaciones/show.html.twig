{% extends 'comunicaciones/comunicaciones.html.twig' %}

{% block title %}Comunicación: {{ entity.nombre }}{% endblock %}
{% block content %}
<div class="container">
    <h3>Comunicación {{ entity.id }}, {{ entity.nombre }}</h3>

    <table class="record_properties">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nombre</th>
            <td>{{ entity.nombre }}</td>
        </tr>
        <tr>
            <th>Descripcion</th>
            <td>{{ entity.descripcion }}</td>
        </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('comunicaciones') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('comunicaciones_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
</div>
{% endblock %}
